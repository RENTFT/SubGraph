{
  "name": "@renft/subgraph",
  "license": "GPL-3.0-or-later",
  "version": "1.0.3",
  "author": "reNFT <hello@renft.io>",
  "scripts": {
    "prepare:mainnet": "mustache config/mainnet.json subgraph.template.yaml > subgraph.yaml",
    "prepare:matic": "mustache config/matic.json subgraph.template.yaml > subgraph.yaml",
    "codegen": "yarn graph codegen",
    "build": "yarn graph build",
    "deploy::mainnet": "yarn prepare:mainnet && graph deploy",
    "deploy::matic": "yarn prepare:matic && graph deploy"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.21.1",
    "@graphprotocol/graph-ts": "0.20.1"
  },
  "devDependencies": {
    "mustache": "4.2.0"
  }
}
